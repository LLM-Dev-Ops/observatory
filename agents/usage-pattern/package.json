{
  "name": "@llm-observatory/usage-pattern-agent",
  "version": "1.0.0",
  "description": "Usage Pattern Agent for LLM-Observatory - READ-ONLY ADVISORY agent for usage trend analysis",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "usage-pattern": "dist/src/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./handler": {
      "types": "./dist/src/handler.d.ts",
      "import": "./dist/src/handler.js"
    },
    "./contracts": {
      "types": "./dist/contracts/schemas.d.ts",
      "import": "./dist/contracts/schemas.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src contracts --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "cli": "node dist/src/cli.js",
    "start": "node dist/src/handler.js"
  },
  "keywords": [
    "llm-observatory",
    "usage-pattern",
    "telemetry",
    "analytics",
    "advisory-agent",
    "read-only"
  ],
  "author": "LLM Observatory Contributors",
  "license": "Apache-2.0",
  "dependencies": {
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/llm-observatory/observatory.git",
    "directory": "agents/usage-pattern"
  },
  "constitution": {
    "classification": "advisory",
    "read_only": true,
    "decision_type": "usage_pattern_analysis",
    "constraints_applied": false,
    "non_responsibilities": [
      "failure_classification",
      "health_evaluation",
      "threshold_enforcement",
      "alert_generation",
      "orchestration_trigger"
    ]
  }
}
