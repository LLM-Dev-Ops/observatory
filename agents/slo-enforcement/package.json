{
  "name": "@observatory/slo-enforcement-agent",
  "version": "1.0.0",
  "description": "SLO/SLA Enforcement Agent - Detects violations of Service Level Objectives and Agreements",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./contracts": {
      "import": "./dist/contracts/index.js",
      "types": "./dist/contracts/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint 'src/**/*.ts' 'contracts/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'contracts/**/*.ts' --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy": "gcloud functions deploy slo-enforcement --gen2 --runtime nodejs20 --trigger-http --entry-point sloEnforcement",
    "dev": "npx tsx --watch src/index.ts"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@google-cloud/functions-framework": "^3.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "slo",
    "sla",
    "enforcement",
    "observability",
    "llm",
    "observatory"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/org/observatory.git",
    "directory": "agents/slo-enforcement"
  },
  "files": [
    "dist",
    "contracts",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
